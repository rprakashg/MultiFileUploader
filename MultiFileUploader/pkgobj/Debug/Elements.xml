<?xml version="1.0" encoding="utf-8"?>
<Elements xmlns="http://schemas.microsoft.com/sharepoint/">
  <CustomAction Id="8c5e6154-548d-4f8b-b8a3-c3ea2f1b4a91.UploadMultipleFilesAction"
                RegistrationType="List"
                RegistrationId="101"
                Location="CommandUI.Ribbon"
                Sequence="10001"
                Title="Upload Multiple Files" 
                HostWebDialog="true" 
                HostWebDialogHeight="500" 
                HostWebDialogWidth="600">
    <CommandUIExtension>
      <!-- 
      Update the UI definitions below with the controls and the command actions
      that you want to enable for the custom action.
      -->
      <CommandUIDefinitions>
        <CommandUIDefinition Location="Ribbon.Documents.New.Controls._children">
          <Button Id="Ribbon.Documents.New.UploadMultipleFilesActionButton"
                  Alt="Upload Multiple Files"
                  Sequence="100"
                  Command="Invoke_UploadMultipleFilesActionButtonRequest"
                  LabelText="Upload Multiple Files"
                  TemplateAlias="o1"
                  Image32by32="data:image/png;base64, "
                  Image16by16="data:image/png;base64, " />
        </CommandUIDefinition>
      </CommandUIDefinitions>
      <CommandUIHandlers>
        <CommandUIHandler Command="Invoke_UploadMultipleFilesActionButtonRequest"
                          CommandAction="~appWebUrl/Pages/Default.aspx?{StandardTokens}&amp;SPListId={SelectedListId}"/>
      </CommandUIHandlers>
    </CommandUIExtension >
  </CustomAction>
</Elements>
